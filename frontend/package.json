{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "main": "public/main.js",
    "dependencies": {
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/react": "^18.3.2",
        "@types/react-dom": "^18.3.0",
        "chart.js": "^4.4.2",
        "react": "^18.3.1",
        "react-chartjs-2": "^5.2.0",
        "react-dom": "^18.3.1",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.4"
